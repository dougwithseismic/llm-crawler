    "prom-client": "^15.1.3",
    "puppeteer": "^22.15.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.7.4",
    "uuid": "^11.0.2",
    "winston": "^3.3.3",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.x.x",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "concurrently": "^9.1.2",
    "ngrok": "5.0.0-beta.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8"
  },
  "name": "gtm-crawler",
  "private": true,
  "scripts": {
    "build": "dotenv -e .env.local -- tsc",
    "build:client": "cd client && pnpm install && pnpm build",
    "build:all": "pnpm build && pnpm build:client",
    "dev:server": "dotenv -e .env.local -- tsx watch src/index.ts",
    "dev:client": "cd client && pnpm dev",
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:client\"",
    "start:server": "dotenv -e .env.local -- node dist/index.js",
    "start:client": "cd client && pnpm preview --port 3000",
    "start": "pnpm build:all && pnpm start:server",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "setup:tunnel": "tsx scripts/setup-tunnel.ts"
  },
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "Doug Silkstone <doug@withseismic.com>",
  "license": "MIT",
  "description": "A local-first crawler for Go To Market teams with big ambitions. Built with TypeScript, Express, and Playwright.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dougwithseismic/gtm-crawler.git"
  },
  "keywords": [
    "gtm",
    "go-to-market",
    "crawler",
